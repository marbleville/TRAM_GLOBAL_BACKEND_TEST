	"bugs": {
		"url": "https://github.com/marbleville/TRAM_GLOBAL_BACKEND_TEST/issues"
	},
	"homepage": "https://github.com/marbleville/TRAM_GLOBAL_BACKEND_TEST#readme",
	"dependencies": {
		"express": "^4.18.2"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"jest": "^29.7.0",
		"supertest": "^6.3.3"
	},
	"jest": {
		"testEnvironment": "node",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		]
	}
